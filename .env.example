# ============================================
# WangSh 生产环境配置示例 (.env.prod.example)
# 复制此文件为 .env.prod 并修改敏感信息
# ============================================

PROJECT_NAME=WangSh
APP_VERSION=1.1.0

DEPLOYMENT_ENV=production
DEBUG=False
LOG_LEVEL=INFO

# 数据库 (PostgreSQL 容器名)
POSTGRES_HOST=postgres
POSTGRES_PORT=5432
POSTGRES_USER=admin
POSTGRES_PASSWORD=prod_postgres_password_change_me
POSTGRES_DB=wangsh_db

# Redis (Redis 容器名)
REDIS_HOST=redis
REDIS_PORT=6379

# 安全 (生产环境务必修改)
SECRET_KEY=prod_change_me_to_a_very_long_random_string
SUPER_ADMIN_PASSWORD=prod_admin_password_change_me
AGENT_API_KEY_ENCRYPTION_KEY=prod_change_me_to_a_base64_key

# 前端
FRONTEND_PORT=6608
WEB_PORT=80
REACT_APP_API_URL=https://your-domain.com/api/v1
CORS_ORIGINS=["https://your-domain.com"]
